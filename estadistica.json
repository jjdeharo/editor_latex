{
  "categorias": [
    {
      "nombre": "Medidas de centralización",
      "id": "centralizacion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\overline{x} = \\frac{\\sum x_i}{n}",
          "title": "Media aritmética"
        },
        {
          "type": "button",
          "latex": "\\overline{x}_p = \\frac{\\sum w_i x_i}{\\sum w_i}",
          "title": "Media ponderada"
        },
        {
          "type": "button",
          "latex": "\\overline{x}_g = \\sqrt[n]{\\prod x_i}",
          "title": "Media geométrica"
        },
        {
          "type": "button",
          "latex": "\\overline{x}_h = \\frac{n}{\\sum \\frac{1}{x_i}}",
          "title": "Media armónica"
        },
        {
          "type": "button",
          "latex": "\\overline{x}_{\\text{cuad}} = \\sqrt{\\frac{\\sum x_i^2}{n}}",
          "title": "Media cuadrática"
        },
        {
          "type": "button",
          "latex": "\\tilde{x}",
          "display": "\\tilde{x}",
          "title": "Mediana"
        },
        {
          "type": "button",
          "latex": "P_k = \\frac{k(n+1)}{100}",
          "title": "Percentil k-ésimo"
        }
      ]
    },
    {
      "nombre": "Medidas de dispersión",
      "id": "dispersion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "R = x_{\\text{max}} - x_{\\text{min}}",
          "title": "Rango"
        },
        {
          "type": "button",
          "latex": "RI = Q_3 - Q_1",
          "title": "Rango intercuartílico"
        },
        {
          "type": "button",
          "latex": "CV = \\frac{\\sigma}{\\overline{x}} \\cdot 100\\%",
          "title": "Coeficiente de variación"
        },
        {
          "type": "button",
          "latex": "\\sigma^2 = \\frac{\\sum (x_i - \\overline{x})^2}{n}",
          "title": "Varianza poblacional"
        },
        {
          "type": "button",
          "latex": "s^2 = \\frac{\\sum (x_i - \\overline{x})^2}{n - 1}",
          "title": "Varianza muestral"
        },
        {
          "type": "button",
          "latex": "\\sigma = \\sqrt{\\frac{\\sum (x_i - \\overline{x})^2}{n}}",
          "title": "Desviación típica poblacional"
        },
        {
          "type": "button",
          "latex": "s = \\sqrt{\\frac{\\sum (x_i - \\overline{x})^2}{n - 1}}",
          "title": "Desviación típica muestral"
        },
        {
          "type": "button",
          "latex": "MAD=\\frac{1}{n}\\sum|x_i-\\overline{x}|",
          "title": "Desviación media absoluta"
        },
        {
          "type": "button",
          "latex": "QD=\\frac{Q_3-Q_1}{2}",
          "title": "Semirrango intercuartílico"
        },
        {
          "type": "button",
          "latex": "z_i = \\frac{x_i - \\overline{x}}{\\sigma}",
          "title": "Valor tipificado (z-score poblacional)"
        },
        {
          "type": "button",
          "latex": "z_i = \\frac{x_i - \\overline{x}}{s}",
          "title": "Valor tipificado (z-score muestral)"
        },
        {
          "type": "button",
          "latex": "\\text{Curtosis}_{\\text{pob}} = \\frac{\\sum (x_i - \\overline{x})^4}{n\\sigma^4}",
          "title": "Curtosis poblacional"
        },
        {
          "type": "button",
          "latex": "\\text{Curtosis}_{\\text{mues}} = \\frac{\\sum (x_i - \\overline{x})^4}{(n - 1)s^4}",
          "title": "Curtosis muestral"
        }
      ]
    },
    {
      "nombre": "Frecuencias",
      "id": "frecuencias",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "f_i = \\frac{n_i}{n}",
          "title": "Frecuencia relativa"
        },
        {
          "type": "button",
          "latex": "F_i = \\sum_{j=1}^{i} f_j",
          "title": "Frecuencia acumulada"
        },
        {
          "type": "button",
          "latex": "N_i = \\sum_{j=1}^{i} n_j",
          "title": "Frecuencia absoluta acumulada"
        }
      ]
    },
    {
      "nombre": "Intervalos",
      "id": "intervalos",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "c_i = \\frac{l_i + l_{i+1}}{2}",
          "title": "Marca de clase"
        },
        {
          "type": "button",
          "latex": "A = l_{i+1} - l_i",
          "title": "Amplitud de clase"
        },
        {
          "type": "button",
          "latex": "Me = L + \\frac{\\frac{n}{2} - F}{f} \\cdot A",
          "title": "Mediana en intervalos"
        },
        {
          "type": "button",
          "latex": "Mo = L + \\frac{(f_1 - f_0)}{(f_1 - f_0)+(f_1 - f_2)} \\cdot A",
          "title": "Moda en intervalos"
        }
      ]
    },
    {
      "nombre": "Momentos y generadoras",
      "id": "momentos",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "E[X]=\\sum x_iP(X=x_i)",
          "title": "Esperanza discreta"
        },
        {
          "type": "button",
          "latex": "E[X]=\\int x f(x)dx",
          "title": "Esperanza continua"
        },
        {
          "type": "button",
          "latex": "Var(X)=E[X^2]-(E[X])^2",
          "title": "Varianza (momentos)"
        },
        {
          "type": "button",
          "latex": "M_X(t)=E[e^{tX}]",
          "title": "Generadora de momentos"
        }
      ]
    },
    {
      "nombre": "Distribuciones de probabilidad",
      "id": "distribuciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "P(X=k)=\\binom{n}{k}p^k(1-p)^{n-k}",
          "title": "Binomial"
        },
        {
          "type": "button",
          "latex": "P(X=k)=\\frac{\\lambda^k e^{-\\lambda}}{k!}",
          "title": "Poisson"
        },
        {
          "type": "button",
          "latex": "f(x)=\\lambda e^{-\\lambda x},\\;F(x)=1-e^{-\\lambda x}",
          "title": "Exponencial"
        },
        {
          "type": "button",
          "latex": "Z \\sim N(0,1)",
          "title": "Distribución normal estándar"
        },
        {
          "type": "button",
          "latex": "t_{n-1}",
          "title": "Distribución t de Student"
        },
        {
          "type": "button",
          "latex": "\\chi^2_k",
          "title": "Distribución chi-cuadrado"
        },
        {
          "type": "button",
          "latex": "F_{k_1,k_2}",
          "title": "Distribución F de Fisher"
        }
      ]
    },
    {
      "nombre": "Coeficientes de correlación",
      "id": "coeficientes_correlacion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\mathrm{Cov}(X,Y)=\\frac{1}{n}\\sum(x_i-\\overline{x})(y_i-\\overline{y})",
          "title": "Covarianza poblacional"
        },
        {
          "type": "button",
          "latex": "s_{XY}=\\frac{1}{n-1}\\sum(x_i-\\overline{x})(y_i-\\overline{y})",
          "title": "Covarianza muestral"
        },
        {
          "type": "button",
          "latex": "r=\\frac{\\sum(x_i-\\overline{x})(y_i-\\overline{y})}{\\sqrt{\\sum(x_i-\\overline{x})^2\\sum(y_i-\\overline{y})^2}}",
          "title": "Pearson"
        },
        {
          "type": "button",
          "latex": "t=r\\sqrt{\\frac{n-2}{1-r^2}}",
          "title": "Test t de Pearson"
        },
        {
          "type": "button",
          "latex": "\\rho=1-\\frac{6\\sum d_i^2}{n(n^2-1)}",
          "title": "Spearman"
        },
        {
          "type": "button",
          "latex": "\\tau=\\frac{C-D}{\\tfrac12n(n-1)}",
          "title": "Tau de Kendall",
          "display": ""
        },
        {
          "type": "button",
          "latex": "\\phi=\\sqrt{\\frac{\\chi^2}{n}}",
          "title": "Coeficiente phi"
        }
      ]
    },
    {
      "nombre": "Regresión",
      "id": "regresion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "y = a + bx",
          "title": "Recta de regresión"
        },
        {
          "type": "button",
          "latex": "b = \\frac{n\\sum xy - \\sum x \\sum y}{n\\sum x^2 - (\\sum x)^2}",
          "title": "Pendiente"
        },
        {
          "type": "button",
          "latex": "a = \\bar{y} - b\\bar{x}",
          "title": "Ordenada al origen"
        },
        {
          "type": "button",
          "latex": "r = \\frac{n\\sum xy - \\sum x \\sum y}{\\sqrt{[n\\sum x^2 - (\\sum x)^2][n\\sum y^2 - (\\sum y)^2]}}",
          "title": "Coeficiente de correlación"
        },
        {
          "type": "button",
          "latex": "R^2 = r^2",
          "title": "Coeficiente de determinación"
        },
        {
          "type": "button",
          "latex": "R^2_{adj}=1-(1-R^2)\\frac{n-1}{n-p-1}",
          "title": "R^2 ajustado"
        },
        {
          "type": "button",
          "latex": "SE_{est}=\\sqrt{\\frac{\\sum(y_i-\\hat y_i)^2}{n-2}}",
          "title": "Error estándar de la estimación"
        },
        {
          "type": "button",
          "latex": "\\hat y\\pm t_{n-2}SE_{est}\\sqrt{1+\\frac1n+\\frac{(x_0-\\overline{x})^2}{\\sum(x_i-\\overline{x})^2}}",
          "title": "Intervalo de predicción"
        }
      ]
    },
    {
      "nombre": "Inferencia estadística",
      "id": "inferencia",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "z = \\frac{\\bar{x} - \\mu}{\\sigma/\\sqrt{n}}",
          "title": "Estadístico z"
        },
        {
          "type": "button",
          "latex": "t = \\frac{\\bar{x} - \\mu}{s/\\sqrt{n}}",
          "title": "Estadístico t"
        },
        {
          "type": "button",
          "latex": "E = z \\cdot \\frac{\\sigma}{\\sqrt{n}}",
          "title": "Error estándar con z"
        },
        {
          "type": "button",
          "latex": "E = t \\cdot \\frac{s}{\\sqrt{n}}",
          "title": "Error estándar con t"
        },
        {
          "type": "button",
          "latex": "\\bar{x} \\pm z \\cdot \\frac{\\sigma}{\\sqrt{n}}",
          "title": "Intervalo con desviación estándar poblacional"
        },
        {
          "type": "button",
          "latex": "\\bar{x} \\pm t \\cdot \\frac{s}{\\sqrt{n}}",
          "title": "Intervalo con desviación muestral"
        },
        {
          "type": "button",
          "latex": "n=\\left(\\frac{z\\,\\sigma}{E}\\right)^2",
          "title": "Tamaño muestra (media)"
        },
        {
          "type": "button",
          "latex": "n=\\frac{z^2\\,\\hat p(1-\\hat p)}{E^2}",
          "title": "Tamaño muestra (proporción)"
        }
      ]
    },
    {
      "nombre": "Estimación de proporciones",
      "id": "proporciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "SE(\\hat p)=\\sqrt{\\frac{\\hat p(1-\\hat p)}{n}}",
          "title": "Error estándar de proporción"
        },
        {
          "type": "button",
          "latex": "\\hat p\\pm z\\sqrt{\\frac{\\hat p(1-\\hat p)}{n}}",
          "title": "IC para proporción"
        }
      ]
    },
    {
      "nombre": "Pruebas de hipótesis",
      "id": "pruebas_hipotesis",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "z=\\frac{\\hat p-p_0}{\\sqrt{\\frac{p_0(1-p_0)}{n}}}",
          "title": "z para proporciones"
        },
        {
          "type": "button",
          "latex": "z=\\frac{\\hat p_1-\\hat p_2}{\\sqrt{\\hat p(1-\\hat p)(1/n_1+1/n_2)}}",
          "title": "z diferencia proporciones"
        },
        {
          "type": "button",
          "latex": "t=\\frac{\\overline{x}-\\mu_0}{s/\\sqrt{n}}",
          "title": "t de una muestra"
        },
        {
          "type": "button",
          "latex": "t=\\frac{\\overline{x}_1-\\overline{x}_2}{\\sqrt{s_p^2(1/n_1+1/n_2)}}",
          "title": "t dos medias indep"
        },
        {
          "type": "button",
          "latex": "s_p^2=\\frac{(n_1-1)s_1^2+(n_2-1)s_2^2}{n_1+n_2-2}",
          "title": "Varianza agrupada"
        },
        {
          "type": "button",
          "latex": "t=\\frac{\\overline{d}}{s_d/\\sqrt{n}}",
          "title": "t apareada"
        },
        {
          "type": "button",
          "latex": "\\chi^2=\\sum\\frac{(O_i-E_i)^2}{E_i}",
          "title": "Chi-cuadrado"
        }
      ]
    },
    {
      "nombre": "ANOVA",
      "id": "anova",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "SS_B=\\sum_{i=1}^k n_i(\\overline{x}_i-\\overline{x}_g)^2",
          "title": "SS entre grupos"
        },
        {
          "type": "button",
          "latex": "SS_W=\\sum_{i=1}^k\\sum_{j=1}^{n_i}(x_{ij}-\\overline{x}_i)^2",
          "title": "SS dentro de grupos"
        },
        {
          "type": "button",
          "latex": "F=\\frac{SS_B/(k-1)}{SS_W/(N-k)}",
          "title": "Estadístico F (ANOVA)"
        }
      ]
    },
    {
      "nombre": "Asociación y contingencia",
      "id": "asociacion",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "C=\\sqrt{\\frac{\\chi^2}{n+\\chi^2}}",
          "title": "Coef contigencia"
        },
        {
          "type": "button",
          "latex": "V=\\sqrt{\\frac{\\chi^2}{n\\min(r-1,c-1)}}",
          "title": "V de Cramer"
        }
      ]
    },
    {
      "nombre": "Desigualdades",
      "id": "desigualdades",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "P(|X-\\mu|\\ge k\\sigma)\\le 1/k^2",
          "title": "Chebyshev"
        }
      ]
    }
  ]
}